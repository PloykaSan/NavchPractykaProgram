#include <stdio.h>

#define MOD 12345

//функція для обчислення к-сті послідовностей з заданою довжиною n
int countSequences(int n) {

//масив для зберігання проміжних результатів
    int dp[n + 1];
    
//початкові значення
    dp[0] = 1; 
    dp[1] = 2;
    dp[2] = 4;

 //заповнення масиву згідно рекурентної формули
    for (int i = 3; i <= n; i++) {
        dp[i] = (dp[i - 1] + dp[i - 2] + dp[i - 3]) % MOD;
    }

//повертаємо к-сть послідовностей
    return dp[n];
}

//назначення змінної, зчитування числа
int main() {
    int n;
    printf("Введіть довжину послідовності n: ");
    scanf("%d", &n);

//перевірка коректності введеного значення n
    while (n <= 1 || n >= 10000) {
        printf("Неправильне введення. Будь ласка, введіть число у діапазоні 1 < n < 10000: ");
        scanf("%d", &n);
    }

//обчислення к-сті послідовностей, виведення результату
    int result = countSequences(n);
    printf("Кількість шуканих послідовностей: %d\n", result);

    return 0;
}
